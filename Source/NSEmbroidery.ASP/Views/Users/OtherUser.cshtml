@model IEnumerable<NSEmbroidery.Data.Models.Embroidery>

@{
    ViewBag.Title = "OtherUser";
    Layout = null;
    @Scripts.Render("~/Scripts/open-image-functionality.js");
    @Scripts.Render("~/Scripts/gallery.js");
    @Styles.Render("~/Content/gallery.css");
}

<h3 class="name-text">@(ViewBag.UserName + "'s gallery")</h3>

<ul id="gallery">
@foreach (var emb in Model)
{
    <li class="gallery-border">
        <div class="gallery-border-content" data-embroidery-id="@emb.Id">
         
            <img src=@Url.Action("ShowImage","Gallery",new { id = emb.Id  }) data-embroidery-id="@emb.Id" />

       </div>
       
        <div class ="embroideries-name">
           @emb.Name
        </div>
    </li>
}
</ul>

@Html.Partial("~/Views/Profile/OpenImage.cshtml")
