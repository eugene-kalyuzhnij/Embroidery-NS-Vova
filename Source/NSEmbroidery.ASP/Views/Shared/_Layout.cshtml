<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        <!--Scripts.Render("~/bundles/modernizr")-->
        @Scripts.Render("~/Scripts/jquery-1.9.0.js")
        @Scripts.Render("~/Scripts/jquery.unobtrusive-ajax.min.js")
        <!--<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>-->
        @Styles.Render("~/Content/layout.css")
        @Scripts.Render("~/Scripts/global.js")
        @Scripts.Render("~/Scripts/jquery-migrate-1.2.1.min.js")
        @Scripts.Render("~/Scripts/jquery.history.js")

    
    </head>
    <body>
        <div class="top">

                    @Html.Partial("_LoginPartial")

                    <ul class="layout-menu">
                       <li class="layout-menu-li">@Html.ActionLink("Home", "Index", "Home", null, new { @class = "layout-menu-a" })</li>
                    </ul>
        </div>

        <div id="render-body">
            @RenderBody()
        </div>

    </body>
</html>
